<?xml version="1.0" encoding="UTF-8"?>
<profiles version="14">
  <profile kind="CodeFormatterProfile" name="Custom Eclipse Formatter" version="14">
    <setting id="org.eclipse.jdt.core.formatter.tabulation.char" value="space"/>
    <setting id="org.eclipse.jdt.core.formatter.tabulation.size" value="4"/>
    <setting id="org.eclipse.jdt.core.formatter.indentation.size" value="4"/>
    <setting id="org.eclipse.jdt.core.formatter.lineSplit" value="120"/>
    <setting id="org.eclipse.jdt.core.formatter.insert_space_after_comma_in_method_arguments" value="true"/>
    <setting id="org.eclipse.jdt.core.formatter.insert_space_before_opening_brace_in_method_declaration" value="true"/>
    <setting id="org.eclipse.jdt.core.formatter.insert_space_after_keywords_in_control_flow" value="true"/>
    <setting id="org.eclipse.jdt.core.formatter.align_type_members_on_columns" value="false"/>
    <setting id="org.eclipse.jdt.core.formatter.comment.line_length" value="100"/>
    <setting id="org.eclipse.jdt.core.formatter.blank_lines_before_package" value="0"/>
    <setting id="org.eclipse.jdt.core.formatter.blank_lines_after_package" value="1"/>
    <setting id="org.eclipse.jdt.core.formatter.blank_lines_before_imports" value="0"/>
    <setting id="org.eclipse.jdt.core.formatter.blank_lines_after_imports" value="1"/>
    <setting id="org.eclipse.jdt.core.formatter.keep_simple_if_on_one_line" value="true"/>
    <setting id="org.eclipse.jdt.core.formatter.never_indent_line_comments_on_first_column" value="true"/>
    <setting id="org.eclipse.jdt.core.formatter.join_wrapped_lines" value="true"/>
  </profile>
</profiles>
